<Page x:Class="ExpressTrack.ExpressListPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExpressTrack"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="ExpressListPage" Initialized="Page_Initialized">

    <Grid>
        <Grid.RowDefinitions>
        </Grid.RowDefinitions>
        <DataGrid x:Name="DG_expressList" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Id}" Header="快递编号"/>
                <DataGridTextColumn Binding="{Binding Name}" Header="快递名称"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Page>
